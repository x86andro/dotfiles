configuration {
	modi: "drun,run,filebrowser,window";
	case-sensitive: false;
	cycle: true;
	filter: "";
	scroll-method: 0;
	normalize-match: true;
	show-icons: true;
	icon-theme: "Papirus";
	steal-focus: false;

	run,drun {
		fallback-icon: "application-x-addon";
	}
}

@theme "~/.config/rofi/themes/andro.rasi"
